          <xs:element name="agentThresholdProfileName" type="CallCenterAgentThresholdProfileName" minOccurs="0"/>
          <xs:element name="agentUnavailableCode" type="CallCenterAgentUnavailableCode" nillable="true" minOccurs="0"/>
